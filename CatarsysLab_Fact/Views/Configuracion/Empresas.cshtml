

@Scripts.Render("~/Plugins/datatTables")


<div class="container">

    <!-- Modal -->

    <div class="modal fade" id="addModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="TitleModalAsignacion">Empresas</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body" id="contAgregar">
                    <form class="form-horizontal" role="form" id="fnGuardar">
                        <input type="hidden" name="idEmpresa" id="idEmpresa" />

                        <div class="row" id="fnNombre">
                            <div class="form-group col-md-12">
                                <label class="col-md-2 control-label" style="width: 12%;">Nombre:</label>
                                <div class="col-md-10" style="width: 88%;">
                                    <input type="text" class="form-control" id="Nombre" name="Nombre" tabindex="1" maxlength="250">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnRFC">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">Razón Social:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="RazonSocial" name="RazonSocial" tabindex="2" maxlength="250">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">RFC:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="RFC" name="RFC" tabindex="3" maxlength="20">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <h5>Dirección fiscal</h5>
                        </div>

                        <div class="row" id="fnCalle">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label">Calle:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="calle" name="calle" tabindex="4" maxlength="50">                                       
                                </div>
                            </div>
                            <div class="form-group col-md-3">
                                <label class="col-md-4 control-label">Exterior:</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" id="Exterior" name="Exterior" tabindex="5" maxlength="8">
                                </div>
                            </div>
                            <div class="form-group col-md-3">
                                <label class="col-md-4 control-label">Interior:</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" id="Interior" name="Interior" tabindex="6" maxlength="8">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnColonia">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">Colonia:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="Colonia" name="Colonia" tabindex="7" maxlength="50">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">CP:</label>
                                <div class="col-md-9">
                                    <input type="number" class="form-control" id="CP" name="CP" tabindex="8" maxlength="5">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnEstadoDomicilio">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">Del / Mpio:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="DelMpio" name="DelMpio" tabindex="9" maxlength="50">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">Estado:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="EstadoDomicilio" name="EstadoDomicilio" tabindex="10" maxlength="25">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnEmail">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">E-mail:</label>
                                <div class="col-md-9">
                                    <input type="email" class="form-control" id="Email" name="Email" tabindex="11" maxlength="50">
                                </div>
                            </div>                          
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>                               
                            </div>
                        </div>

                        <div class="row" id="fnFecha">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label">Fecha Creación:</label>
                                <div class="col-md-9">
                                    <div class="input-group">
                                        <input type="text" class="form-control" placeholder="dd/mm/yyyy" id="fecha_creacion" name="fecha_creacion" tabindex="12">
                                        <span class="input-group-addon bg-primary b-0 text-white"><i class="ti-calendar"></i></span>
                                    </div><!-- input-group -->
                                </div>
                            </div>                         
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                            </div>                                
                        </div>

                        <div class="row" id="fnEstado">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label">Estado:</label>
                                <div class="col-md-9">
                                    <div class="contEstado col-md-6  col-centered">
                                        <label class="radio-inline">
                                            <input type="radio" name="estado" tabindex="13" value="1" id="rdSI">SI
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" checked name="estado" tabindex="14" value="0" id="rdNO">NO
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>                               
                            </div>
                        </div>


                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                    <button type="button" class="btn btn-primary" id="Guardar">Guardar</button>
                </div>
            </div>
        </div>
    </div>

    <!-- ENd Modal-->
    <!-- Page-Title -->
    <div class="row">
        <div class="col-sm-12">
            <div class="card-box table-responsive">

                <h4 class="header-title m-t-0 m-b-30">Empresas</h4>

                <table id="datatable" class="table table-striped dt-responsive nowrap dataTable no-footer dtr-inline" role="grid" aria-describedby="datatable_info">
                    <thead>
                        <tr>
                            <th>IdEmpresa</th>
                            <th>Nombre</th>
                            <th>Razón Social</th>
                            <th>Fecha de Creación</th>
                            <th>Estado</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div><!-- end col -->
    </div>
    <!-- end row -->

    <script src="~/Scripts/Empresas.js"></script>

</div> <!-- container -->
