

@Scripts.Render("~/Plugins/datatTables")


<div class="container">

    <!-- Modal Contacto -->

    <div class="modal fade" id="addModalContacto" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="TitleModalAsignacion">Contacto</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body" id="contAgregar">
                    <form class="form-horizontal" role="form" id="fnContacto">

                        <input type="hidden" name="idContacto" id="idContacto" />
                        <input type="hidden" name="idClienteContacto" id="idClienteContacto" />

                        <div class="row" id="fnNombre_Contacto">
                            <div class="form-group col-md-6">
                                <label class="col-md-2 control-label">Nombre:</label>
                                <div class="col-md-10">
                                    <input type="text" class="form-control" id="Nombre_Contacto" name="Nombre_Contacto" tabindex="1" maxlength="50">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-md-2 control-label">Puesto:</label>
                                <div class="col-md-10">
                                    <input type="text" class="form-control" id="Puesto_Contacto" name="Puesto_Contacto" tabindex="2" maxlength="50">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnEmail_Contacto">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">E-mail:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="Email_Contacto" name="Email_Contacto" tabindex="3" maxlength="30">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">Skype:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="Skype_Contacto" name="Skype_Contacto" tabindex="4" maxlength="20">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnTelefono_Contacto">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label">Teléfono:</label>
                                <div class="col-md-9">
                                    <input type="number" class="form-control" id="Telefono_Contacto" name="Telefono_Contacto" tabindex="5" maxlength="15">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-md-4 control-label">Móvil:</label>
                                <div class="col-md-8">
                                    <input type="number" class="form-control" id="Movil_Contacto" name="Movil_Contacto" tabindex="6" maxlength="15">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnComentario_Contacto">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label">Comentarios:</label>
                                <div class="col-md-9">
                                    <textarea class="form-control" id="Comentario_Contacto" name="Comentario_Contacto" tabindex="7" maxlength="250"></textarea>
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnEstado">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label">Estado:</label>
                                <div class="col-md-9">
                                    <div class="contEstado col-md-6  col-centered">
                                        <label class="radio-inline">
                                            <input type="radio" name="estado" tabindex="13" value="1" id="rdCtSI">SI
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" checked name="estado" tabindex="14" value="0" id="rdCtNO">NO
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label">Envío de Factura:</label>
                                <div class="col-md-9">
                                    <div class="contEstado col-md-6  col-centered">
                                        <label class="radio-inline">
                                            <input type="radio" name="enviaFactura" tabindex="13" value="1" id="efSI">SI
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" checked name="enviaFactura" tabindex="14" value="0" id="efNO">NO
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal" id="CancelarContacto">Cancelar</button>
                    <button type="button" class="btn btn-primary" id="GuardarContacto">Guardar</button>
                </div>
            </div>
        </div>
    </div>


    <!-- Modal -->

    <div class="modal fade" id="addModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="TitleModalAsignacion">Clientes</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body" id="contAgregar">
                    <form class="form-horizontal" role="form" id="fnGuardar">
                        <input type="hidden" name="idCliente" id="idCliente" />
                        <input type="hidden" name="frIdEmpresa" id="frIdEmpresa" />

                        <div class="row" id="fnNombre">
                            <div class="form-group col-md-12">
                                <label class="col-md-2 control-label" style="width: 12%;">Nombre:</label>
                                <div class="col-md-10" style="width: 88%;">
                                    <input type="text" class="form-control" id="Nombre" name="Nombre" tabindex="1" maxlength="100">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnRFC">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">Razón Social:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="RazonSocial" name="RazonSocial" tabindex="2" maxlength="100">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">RFC:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="RFC" name="RFC" tabindex="3" maxlength="13">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <h5>Dirección fiscal</h5>
                        </div>

                        <div class="row" id="fnCalle">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label">Calle:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="calle" name="calle" tabindex="4" maxlength="50">
                                </div>
                            </div>
                            <div class="form-group col-md-3">
                                <label class="col-md-4 control-label">Exterior:</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" id="Exterior" name="Exterior" tabindex="5" maxlength="10">
                                </div>
                            </div>
                            <div class="form-group col-md-3">
                                <label class="col-md-4 control-label">Interior:</label>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" id="Interior" name="Interior" tabindex="6" maxlength="10">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnColonia">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">Colonia:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="Colonia" name="Colonia" tabindex="7" maxlength="50">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">CP:</label>
                                <div class="col-md-9">
                                    <input type="number" class="form-control" id="CP" name="CP" tabindex="8" maxlength="5">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnEstadoDomicilio">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">Del / Mpio:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="DelMpio" name="DelMpio" tabindex="9" maxlength="50">
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">Estado:</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="EstadoDomicilio" name="EstadoDomicilio" tabindex="10" maxlength="25">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg2"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnTablaContactos">
                            <div class="col-xs-12">
                                <div class="card-box table-responsive">
                                    <table id="datatableContactos" class="table table-striped dt-responsive nowrap dataTable no-footer dtr-inline" role="grid" aria-describedby="datatable_info">
                                        <thead>
                                            <tr>
                                                <th>Nombre</th>
                                                <th>Puesto</th>
                                                <th>Email</th>
                                                <th>Móvil</th>
                                                <th>Envíar factura</th>
                                                <th>Comentarios</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-sm-12">
                                <button type="button" id="agregarAsgContacto" class="btn btn-custom dropdown-toggle waves-effect waves-light" data-toggle="modal" data-target="#addModalContacto">
                                    Agregar Contacto
                                    <span class="m-l-5">
                                        <i class="fa fa-plus-circle"></i>
                                    </span>
                                </button>
                            </div>
                        </div>



                        <div class="row" id="fnDiasPago">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label" for="example-email">Días de pago:</label>
                                <div class="col-md-9">
                                    <input type="email" class="form-control" id="diasPago" name="diasPago" tabindex="11" maxlength="50">
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                            </div>
                        </div>

                        <div class="row" id="fnEstado">
                            <div class="form-group col-md-6">
                                <label class="col-md-3 control-label">Estado:</label>
                                <div class="col-md-9">
                                    <div class="contEstado col-md-6  col-centered">
                                        <label class="radio-inline">
                                            <input type="radio" name="estado" tabindex="13" value="1" id="rdSI">SI
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" checked name="estado" tabindex="14" value="0" id="rdNO">NO
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <div class="col-xs-6 ">
                                    <div class="col-xs-3"></div>
                                    <div class="col-xs-9 errorMsg"></div>
                                </div>
                            </div>
                        </div>


                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                    <button type="button" class="btn btn-primary" id="Guardar">Guardar</button>
                </div>
            </div>
        </div>
    </div>

    <!-- ENd Modal-->
    <!-- Page-Title -->
    <div class="row">
        <div class="col-sm-12">
            <div class="card-box table-responsive">

                <h4 class="header-title m-t-0 m-b-30">Clientes</h4>

                <table id="datatable" class="table table-striped dt-responsive nowrap dataTable no-footer dtr-inline" role="grid" aria-describedby="datatable_info">
                    <thead>
                        <tr>
                            <th>Id_Cliente</th>
                            <th>Nombre</th>
                            <th>Razón Social</th>
                            <th>RFC</th>
                            <th>Contacto</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div><!-- end col -->
    </div>
    <!-- end row -->

    <script src="~/Scripts/Clientes.js"></script>

</div> <!-- container -->
